
public class timeOffNode {
	private int reqID;
	private String dateRequested;
	private int empID;
	private int timeOffTypeID;
	private char decision;
	private String startDate;
	private String endDate;
	private int totalDaysOff;
	private String notes;
	
	private timeOffNode link;

	public timeOffNode(int reqID, String dateRequested, int empID, int timeOffTypeID, char decision, String startDate,
			String endDate, int totalDaysOff, String notes) {
		this.reqID = reqID;
		this.dateRequested = dateRequested;
		this.empID = empID;
		this.timeOffTypeID = timeOffTypeID;
		this.decision = decision;
		this.startDate = startDate;
		this.endDate = endDate;
		this.totalDaysOff = totalDaysOff;
		this.notes = notes;
		
		this.link = null;
	}

	public int getReqID() {
		return reqID;
	}

	public void setReqID(int reqID) {
		this.reqID = reqID;
	}

	public String getDateRequested() {
		return dateRequested;
	}

	public void setDateRequested(String dateRequested) {
		this.dateRequested = dateRequested;
	}

	public int getEmpID() {
		return empID;
	}

	public void setEmpID(int empID) {
		this.empID = empID;
	}

	public int getTimeOffTypeID() {
		return timeOffTypeID;
	}

	public void setTimeOffTypeID(int timeOffTypeID) {
		this.timeOffTypeID = timeOffTypeID;
	}

	public timeOffNode getLink() {
		return link;
	}

	public void setLink(timeOffNode link) {
		this.link = link;
	}
	 
	
	
	
	
}
